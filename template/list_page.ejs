    <!-- begin :: TabulatorJS -->
    <link href="https://unpkg.com/tabulator-tables/dist/css/tabulator_bulma.min.css" rel="stylesheet">
    <script src="https://unpkg.com/tabulator-tables/dist/js/tabulator.min.js"></script>
    <script>
      /* 등록 버튼 hidden 상태 */
      const btn_visible = <%- JSON.stringify(regist_visible) %>;
      if (!btn_visible) {
        const btn_container = document.getElementById('btn_container')
        btn_container.hidden = true;
      }else{
        btn_container.hidden = false;
      }
      console.log(" btn_visible: ", btn_visible)

      const tableData = <%- JSON.stringify(objectList) %>;
      console.log('tableData: ', tableData);

      const no_data_alert = document.getElementById("no_data_alert")
      const tabulator_container = document.getElementById("tabulator_container")
      console.log("no_data_alert: ", no_data_alert)
      console.log("tabulator_container: ", tabulator_container)
      
      // 빈 값
      if (!tableData || tableData.length <= 0) {
        console.log("if (!tableData || tabulator.length <= 0)" )
        no_data_alert.hidden=false
        tabulator_container.hidden=true
      } else {
        no_data_alert.hidden=true
        tabulator_container.hidden=false
        const table = new Tabulator("#tabulator_container", {
        data: tableData, //테이블에 로드할 데이터s
        layout : "fitColumns",
        pagination:"local", //enable local pagination.
        paginationSize:10, // 한 페이지에 5개의 결과 표시
        columns:<%- JSON.stringify(tabulator_config) %>
      });
      
      table.on("rowClick", function(e, row){ 
        console.log('clicked row data: ', row.getData())
        const edit_url = <%- JSON.stringify(edit_url) %>;
        location.href = edit_url + row.getData().id
      });

      // 검색 필드 이벤트 리스너 설정
      document.getElementById("search_input").addEventListener("input", function(){
          let val = this.value;
          const filter_column = <%- JSON.stringify(filter_column) %>
          table.setFilter(filter_column, "like", val); // 'name' 컬럼에서 입력 값과 유사한 내용을 필터링
      });
      }


    </script>
    <!-- end :: TabulatorJS -->
