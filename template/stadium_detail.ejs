    <!-- 사용자 정의 javascript -->
    <script>
      // 하위 지역 전역 변수
      let g_sub_region_value = 0;

      /* 메인 지역 변경시 하위 지역 리스트 변경하는 onchange 함수 */
      function mainRegionChange(e){
          const url = 'http://127.0.0.1:7070/stadium/subRegion/'+e.target.value;
          const options = {
              method: 'GET', // HTTP 메서드 설정
              headers: {
              'Content-Type': 'application/json', // 요청의 콘텐츠 타입 헤더
              }
          };

          fetch(url, options)
              .then(response => response.json())
              .then(data => {
              console.log('main_region onchange fetch response: ', data)
              const subRegionElement = document.getElementById('sub_region')
              subRegionElement.innerHTML = ''
              const defaultOptionTag = document.createElement('option')
              defaultOptionTag.textContent = 'Select'
              subRegionElement.appendChild(defaultOptionTag)
              data.data.forEach((item, idx) => {
                  const optionElement = document.createElement('option')
                  optionElement.value = idx
                  optionElement.textContent = item
                  subRegionElement.appendChild(optionElement)
              });
              if(g_sub_region_value > 0){
                  document.getElementById('sub_region').value = g_sub_region_value;
                  g_sub_region_value = 0;
              }
              })
              .catch(error => console.error('Error:', error));
      }

      /* ejs파라미터를 화면에 뿌려주는 코드 */
      const stadium_data = <%- JSON.stringify(data) %>;
      console.log('stadium_data: ', stadium_data);
      const config_data = <%- JSON.stringify(config_data) %>;
      console.log('config_data: ', config_data);
      
      // data가 있을 때만 input들에 값 할당
      if(stadium_data){
        const keyArray = Object.keys(stadium_data);
        keyArray.forEach(key => {
          const input_object = document.getElementById(key);

          // photo_path인 경우 value가 아니라 style로 사진경로 부여
          if(input_object && key === 'photo_path') input_object.style = `background-image: url(${stadium_data[key]})`
          
          // 메인지역의 경우 설정했을 때 서브지역이 업데이트 되도록 onchange 발생시키기
          else if(input_object && key === 'main_region') {
            input_object.value = stadium_data[key];
            var event = new Event('change', {
              bubbles: true,    // 이벤트가 버블링되도록 설정
              cancelable: true,  // 이벤트를 취소할 수 있도록 설정
            });
            g_sub_region_value = stadium_data['sub_region'];
            input_object.dispatchEvent(event);
          }

          // getElementById로 검색되는 일반적인 input에 값 대입
          else if(input_object) input_object.value = stadium_data[key];

          // id값을 hidden input에 할당
          else if (!input_object && key === 'id') document.getElementById('stadium_id').value = stadium_data[key];

          // radio 버튼인 경우
          else if(!input_object){
            const radio_list = document.getElementsByName(key)
            if(radio_list.length > 0){
              radio_list.forEach(element => {
                if(element.value == stadium_data[key]) element.checked = true
              })
            }
          }
        })
      }

      // config_data에 값이 있을 때만 값 할당
      if(config_data && config_data.length > 0){
        const config_keys = ['match_type_', 'allow_gender_', 'level_criterion_'];
        config_data.forEach((item, idx) => {
          config_keys.forEach(key => {
            document.getElementById(key+idx).value = item[key.slice(0, -1)]
          })
        })
      }

      /* form submit 등록/수정 분기 처리 */
      document.getElementById('kt_form').addEventListener('submit', function(event) {
        
        // 기본 제출 동작 예방
        event.preventDefault();

        const form = event.target;
        const formData = new FormData(form);
        formData.append('requestTable', 'stadium');
        // FormData의 내용을 로깅
        for (let [key, value] of formData.entries()) {
            console.log(`${key}: ${value}`);
        }

        let target_url = "";

        // stadium_id 유무에 따라 url을 등록/수정 분기하여 요청
        const stadium_id = document.getElementById('stadium_id').value;
        if(stadium_id){
          target_url = `http://127.0.0.1:7070/stadium/edit/${stadium_id}`
        }else{
          target_url = `http://127.0.0.1:7070/stadium/regist`
        }
        console.log('target_url: ', target_url)

        fetch(target_url, {
            method: 'POST',
            body: formData
        })
        .then(response => response.json())
        .then(data => {
          console.log('response data: ', data);
          // status : true 응답일 때
          if(data.status) location.href = `${data.url}`
          // status : false 응답일 때
          else alert('예외가 발생했습니다!\n'+data.error)
        })
        .catch(error => console.error('Error:', error));
      });
    </script>
  </body>
