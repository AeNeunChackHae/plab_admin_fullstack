    <script>
      /* ejs파라미터를 화면에 뿌려주는 코드 */
      document.addEventListener("DOMContentLoaded", function() {
      const user_data = <%- JSON.stringify(data) %>;
      console.log('user_data: ', user_data);

      // data가 있을 때만 input들에 값 할당
      if(Object.keys(user_data).length > 0){
        document.getElementById('user_id').value = user_data.id;
        document.getElementById('username').value = user_data.username;
        document.getElementById('birth_date').value = user_data.birth_date;
        document.getElementById('email').value = user_data.email;
        document.getElementById('phone_number').value = user_data.phone_number;
        document.getElementById('status_code').value = user_data.status_code;
        document.getElementById('level_code').value = user_data.level_code;
        
        const radio_list = document.getElementsByName("gender");
        if(radio_list.length > 0){
          radio_list.forEach(element => {
            if(element.value == user_data["gender"]) element.checked = true
          })
        }
      }

      /* form submit 등록/수정 분기 처리 */
      document.getElementById('kt_form').addEventListener('submit', function(event) {
        
        // 기본 제출 동작 예방
        event.preventDefault();

        const form = event.target;
        const formData = new FormData(form);
        formData.append('requestTable', 'user');
        // FormData의 내용을 로깅
        for (let [key, value] of formData.entries()) {
            console.log(`${key}: ${value}`);
        }

        let target_url = "";

        // user_id 유무에 따라 url을 등록/수정 분기하여 요청
        const user_id = document.getElementById('user_id').value;
        if(user_id){
          target_url = `http://127.0.0.1:7070/user/edit/${user_id}`
        }else{
          target_url = `http://127.0.0.1:7070/user/regist`
        }
        console.log('target_url: ', target_url)
        fetch(target_url, {
            method: 'POST',
            body: formData
        })
        .then(response => response.json())
        .then(data => {
          console.log('response data: ', data);
          // status : true 응답일 때
          if(data.status) location.href = `${data.url}`
          // status : false 응답일 때
          else alert('예외가 발생했습니다!\n'+data.error)
        })
        .catch(error => console.error('Error:', error));
      });
    });
    
    </script>
